{
    "name": "phlak/cafelias",
    "description": "Generate random codenames based on coffee drinks",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "support": {
        "issues": "https://github.com/PHLAK/Cafelias/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "tightenco/collect": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "psy/psysh": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Cafelias\\": "src/"
        },
        "files": ["src/Support/helpers.php"]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
